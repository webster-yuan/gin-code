    ],
    "swagger": "2.0",
    "info": {
        "description": "这是一个基于Gin框架的企业级Go项目API",
        "title": "Go Startup API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
